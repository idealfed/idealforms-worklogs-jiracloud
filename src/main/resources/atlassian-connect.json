{
  "key": "${addon.key}",
  "baseUrl": "${addon.base-url}",
  "name": "Com Idealfed Forms",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "scopes": [
    "READ",
    "WRITE",
    "DELETE",
    "ADMIN",
    "ACT_AS_USER"
  ],
  "modules": {
    "generalPages": [
      {
        "key": "ijf-worklogs-runtime",
        "location": "system.top.navigation.bar",
        "name": {
          "value": "Ideal Worklogs"
        },
        "url": "/run?formId=IFT%20Worklog%20Report",
        "weight": 5,
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ]
      },
      {
        "key": "ijf-10-page",
        "location": "system.admin/globalsettings",
        "name": {
          "value": "Ideal Forms 10"
        },
        "url": "/splash",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ]
      },
      {
        "url": "/run?formId=Worklog%20Report",
        "location": "system.admin/globalsettings",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ],
        "weight": 100,
        "name": {
          "value": "Ideal Worklogs 10"
        },
        "key": "ideal-worklogs-10-general"
      }
    ],
    "webSections": [
      {
        "key": "ift-menu-section",
        "location": "admin_plugins_menu",
        "name": {
          "value": "Ideal Tech"
        }
      }
    ],
    "webItems": [
      {
        "key": "ift-forms-section-link",
        "location": "admin_plugins_menu/ift-menu-section",
        "name": {
          "value": "Ideal Forms"
        },
        "url": "/splash",
        "target": {
          "type": "page"
        }
      },
      {
        "key": "ift-worklogs-section-link",
        "location": "admin_plugins_menu/ift-menu-section",
        "name": {
          "value": "Ideal Worklogs"
        },
        "url": "/run?formId=IFT%20Product%20View",
        "target": {
          "type": "page"
        }
      }
    ]
  }
}


