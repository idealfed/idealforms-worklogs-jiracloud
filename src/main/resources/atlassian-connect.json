{
  "key": "${addon.key}",
  "baseUrl": "${addon.base-url}",
  "name": "Com Idealfed Forms JDH Branch",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/app-installed",
    "uninstalled": "/app-uninstalled",
    "enabled": "/app-enabled",
    "disabled": "/app-disabled"
  },
  "scopes": [
    "READ",
    "WRITE",
    "DELETE",
    "ADMIN",
    "ACT_AS_USER"
  ],
  "modules": {
    "jiraSearchRequestViews": [
      {
        "url": "/run?formId=Worklog%20Report",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ],
        "weight": 100,
        "name": {
          "value": "Ideal Worklogs 20"
        },
        "key": "ideal-worklogs-search"
      }
    ],
    "generalPages": [
      {
        "key": "ijf-20-page",
        "location": "system.admin/globalsettings",
        "name": {
          "value": "Ideal Forms 20"
        },
        "url": "/splash",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ]
      },
      {
        "url": "/run?formId=Worklog%20Report",
        "location": "system.admin/globalsettings",
        "conditions": [
          {
            "condition": "user_is_logged_in"
          }
        ],
        "weight": 100,
        "name": {
          "value": "Ideal Worklogs 20"
        },
        "key": "ideal-worklogs-20-general"
      }
    ],
    "webSections": [
      {
        "key": "ift-menu-section-20",
        "location": "admin_plugins_menu",
        "name": {
          "value": "Ideal Tech 20"
        }
      }
    ]
  }
}


